{
  "name": "wyrihaximus/ratchet-commands",
  "description": "Brings 2 way communication to wyrihaximus/ratchet.",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp"
  ],
  "homepage": "http://github.com/WyriHaximus/RatchetCommands",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan kiewiet",
      "email": "ceesjank@gmail.com",
      "role": "Maintainer",
      "homepage": "http://wyrihaximus.net/"
    }
  ],
  "require": {
      "php": ">=5.4.0",
      "composer/installers": "*",
      "wyrihaximus/ratchet": "^0.1"
  },
  "require-dev": {
    "predis/predis": "0.8.*"
  },
  "suggest": {
      "predis/predis": "Send commands to the Ratchet service over redis",
      "predis/predis-async": "Send commands to the Ratchet service over redis",
      "react/zmq": "Send commands to the Ratchet service over 0MQ",
      "dnode/dnode": "Send commands to the Ratchet service over simple pure PHP sockets"
  },
  "support": {
    "issues": "https://github.com/WyriHaximus/RatchetCommands/issues",
    "source": "https://github.com/WyriHaximus/RatchetCommands"
  },
  "extra": {
    "installer-name": "RatchetCommands"
  },
  "config": {
    "vendor-dir": "Vendor"
  }
}
